<div class="card bg-secondary mb-3 border-secondary mx-auto mt-5">
    <div class="card-body">
        <img src= "/<%= post.imageUrl%>" alt="image" style="height: 250px;
        width: 250px;">
    </div>
    <div class="card-header">
        <h3 class="text-center mt-3"><%= post.title%></h3>
        <h4 class="text-center mt-3">
            <%= post.content%>
        </h4>
        <h6 class="text-center mt-3">
            <%= post.creator.username%>
        </h6>
    </div>
</div>